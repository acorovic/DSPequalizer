void calculateShelvingCoeff(float c_alpha, Int16* output)
{
	/* Your code here */
	a = (Int16) alpha * SCALE;

	output[0] = a;
	output[1] = -SCALE - 1;
	output[2] = SCALE;
	output[3] = -a-1;
}

